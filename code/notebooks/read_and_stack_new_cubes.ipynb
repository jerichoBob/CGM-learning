    "import os\n",
    "\n",
    "def find_fits(basedir, endswith):\n",
    "    return (os.path.join(root, file)\n",
    "        for root, dirs, files in os.walk(basedir)\n",
    "            for file in files if root == basedir and file.lower().endswith(endswith))"
